[[Ex([x1,x2,y1,y2,m,h2,w],And(Not(And(0<=w,w<=1)),c1*(c1-1)+h2*c2=0,(m*c1-1)*c1+m*c2^2=0,((1+w*(c1-1))-x1)^2+(w*c2-x2)^2=((1+w*(c1-1))+x1-2*c1)^2+(w*c2+x2)^2,((1+w*(c1-1))+x1-2*c1)^2+(w*c2+x2)^2=((1+w*(c1-1))+x1-2)^2+(w*c2+x2)^2,((1+w*(c1-1))-y1)^2+(w*c2-y2)^2=((1+w*(c1-1))+y1-2*m*c1)^2+(w*c2+y2-2*m*c2)^2,((1+w*(c1-1))+y1-2*m*c1)^2+(w*c2+y2-2*m*c2)^2=((1+w*(c1-1))+y1-2*c1)^2+(w*c2+y2-2*c2)^2,(y1-c1)*(y2-x2)-(y2-h2)*(y1-x1)<>0)),Or(c1-1=0,And(c2<>0,c1^2+c2^2 -c1=0))]]: